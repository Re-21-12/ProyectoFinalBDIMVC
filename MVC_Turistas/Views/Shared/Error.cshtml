@model MVC_Turistas.Models.ErrorViewModel

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Error</title>
</head>
<body>
<h1>Ocurrió un error</h1>
<p>@Model.ErrorMessage</p>
@if (Model.ShowRequestId)
{
    <p>
        <strong>Request ID:</strong> <code>@Model.RequestId</code>
    </p>
}
</body>
</html>